<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitWidth="1230.0" fitHeight="693.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Image/EndWin.png" />
         </image>
      </ImageView>
      <Button layoutX="0.1" layoutY="380.0" mnemonicParsing="false" onAction="#btnRestart" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitWidth="328.0" fitHeight="184.8" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Image/RestartButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="0.1" layoutY="480.0" mnemonicParsing="false" onAction="#btnQuit" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitWidth="328.0" fitHeight="184.8" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Image/QuitButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="wonPlayer" layoutX="550" layoutY="20" textFill="#470e04" text="">
         <font>
            <Font name="Castellar" size="40.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
